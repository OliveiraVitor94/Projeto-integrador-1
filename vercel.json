{
    "version": 2,
    "builds": [
      {
        "src": "projetoIntegrador1/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "templates/static/(.*)",
        "dest": "templates/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "projetoIntegrador1/wsgi.py"
      }
    ]
  }
  